{
    "version": 2,
    "routes": [
      {
        "src": "/encrypt/(.*)",
        "dest": "app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "/",
        "use": "@vercel/static-build"
      }
    ]
  }
  